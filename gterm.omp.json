{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "var": {
    "background": "",
    "color": "#fff",
    "computer": "Computer",
    "logotype": "ganto",
    "showAll": false,
    "showCustom": false
  },
  "secondary_prompt": {
    "template": "➜ ",
    "foreground": "#aaa",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Var.showAll }}{{ if .Var.showCustom }}{{ .Var.logotype }}@{{ .Var.computer }}{{ else }}{{ .UserName }}@{{ .HostName }}{{ end }} {{ end }}",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "{{ .Path }} ",
          "foreground": "#7400ff",
          "background": "transparent",
          "type": "path",
          "style": "plain"
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "transparent",
          "background": "#00add8",
          "template": "Go"
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "transparent",
          "background": "#00add8",
          "template": " v{{.Full}}"
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "transparent",
          "background": "#f74c00",
          "template": "Rust"
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "transparent",
          "background": "#f74c00",
          "template": " v{{.Full}}"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
          "foreground": "#F3C267",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " / ",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "x{{ reason .Code }}",
          "foreground": "#395222",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " / ",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ccc{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "roundrock"
          },
          "template": "run:{{ .FormattedMs }}",
          "foreground": "#395222",
          "type": "executiontime",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " / ",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "template": "{{ .CurrentDate | date .Format }}",
          "foreground": "#414142",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "!",
          "foreground": "#E06C75",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "➜",
          "foreground": "#E06C75",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
