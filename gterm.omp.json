{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "version": 2,
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "-> "
  },
  "var": {
    "logotype": "ganto",
    "background": "",
    "color": "#fff",
    "computer": "Computer"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "background": "{{ .Var.background }}",
          "foreground": "{{ .Var.color }}",
          "template": "{{ .Var.logotype }}@{{ .Var.computer }} "
        },
        {
          "background": "transparent",
          "foreground": "#7400ff",
          "properties": {
            "style": "full"
          },
          "type": "path",
          "style": "plain",
          "template": "{{ .Path }}"
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#00add8",
          "template": " Go"
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#00add8",
          "template": "{{.Full}} "
        },
        {
          "foreground": "#F3C267",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "filler": "",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#395222",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ],
          "template": " x{{ reason .Code }}"
        },
        {
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ccc{{ end }}"
          ],
          "template": " / "
        },
        {
          "foreground": "#395222",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ],
          "properties": {
            "style": "roundrock",
            "always_enabled": true
          },
          "style": "diamond",
          "template": "run:{{ .FormattedMs }}",
          "type": "executiontime"
        },
        {
          "type": "text",
          "style": "plain",
          "template": " / "
        },
        {
          "type": "time",
          "foreground": "#414142",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date .Format }}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "\u0021",
          "type": "root"
        },
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        }
      ]
    }
  ]
}
